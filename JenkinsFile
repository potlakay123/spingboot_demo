pipeline{

agent any
 stages{
   stage('compile stage'){
     steps{
          withMaven(maven :'Maven 3.3'){
         sh 'mvn clean install'		
     }	
     } 
   }
	 stage('Testing stage'){
	   steps{
	    withMaven(maven :'Maven 3.3'){
		 sh 'mvn test'
		}
	   }
	 }

     stage('deployment stage'){
         steps{
             withMaven(maven :'Maven 3.3'){
                 sh 'mvn deploy'
             }
         }
     }
 }

}
