pipeline{

agent any
 stages{
   stage('compile stage'){
     steps{
         bat ('mvn clean install')		
     
     } 
   }
	 stage('Testing stage'){
	   steps{
		  bat ('mvn clean install')
	   }
	 }

     stage('deployment stage'){
         steps{
                  bat ('mvn deploy')
         }
     }
 }

}
